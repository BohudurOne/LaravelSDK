{
    "name": "bohudur/bohudur",
    "description": "Laravel module for Bohudur Payment Gateway",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\Modules\\Bohudur\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Modules\\Bohudur\\BohudurServiceProvider"
            ],
            "aliases": {
                "Bohudur": "App\\Modules\\Bohudur\\Facades\\Bohudur"
            }
        }
    }
}